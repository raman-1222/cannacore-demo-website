{
    "version": 2,
    "functions": {
        "api/index.js": {
            "maxDuration": 60,
            "memory": 3008
        }
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "public/$1"
        }
    ],
    "env": {
        "LAMATIC_API_KEY": "@lamatic_api_key",
        "LAMATIC_WORKFLOW_ID": "@lamatic_workflow_id"
    }
}